@model List<Infoblog.Models.EducationPostModel>


@{
    ViewBag.Title = "EducationPostView";
}

<h2>Utvecklingsarbeten inom utbildning</h2>

@{Html.RenderAction("_NewPostPartial", "Education");}

<div class="col-lg-12">
    @foreach (var p in Model)
    {
    <div class="row">
        <h2>@p.Title</h2>
        <p>@p.Content</p>
        <span class="text-info">@p.Author</span> -
        <span class="text-info">@p.Date</span>

    </div>
    }
</div>



