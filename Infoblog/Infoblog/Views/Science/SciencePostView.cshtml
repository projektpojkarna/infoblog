@{
    ViewBag.Title = "SciencePostView";
}

<h2>Utvecklingsarbeten inom forskning</h2>

@{Html.RenderAction("NewSciencePostPartial", "Science"); }

<div>

    @foreach (var post in Model)
    {
        <div>
            <h2>@post.Title</h2>
            <p>@post.Content</p>
            <span>@post.Author</span>
            <span>@post.Date</span>
        </div>

    }

</div>

